    "simpl-schema": "^1.8.0"
  },
  "devDependencies": {
    "@types/meteor": "^1.4.47",
    "@types/mocha": "^5.2.7",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "autoprefixer": "^9.8.4",
    "css-mqpacker": "^7.0.0",
    "postcss-csso": "^4.0.0",
    "typescript": "^3.8.2"
